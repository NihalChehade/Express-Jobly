{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30,
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Conservator, furniture"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "minimum": 0,
            "title": "The salary Schema",
            "examples": [
                110000
            ]
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "minimum": 0,
            "maximum": 1,
            "title": "The equity Schema",
            "examples": [
                0.5
            ]
        },
        "company_handle": {
            "type": "string",
            "minLength": 1,
            "maxLength": 10,            
            "title": "The company_handle Schema",
            "examples": [
                "watson-davis"
            ]
        }
    },
    "examples": [{
        "title": "Conservator, furniture",
        "salary": 110000,
        "equity": 0.5,
        "company_handle": "watson-davis"
    }]
}